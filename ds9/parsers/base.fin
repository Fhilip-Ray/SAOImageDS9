# INT
[+-]?{D}+ {set yylval $yytext; return $INT_}

# REAL
[+-]?{D}+"."?({E})? |
[+-]?{D}*"."{D}+({E})? {set yylval $yytext; return $REAL_}

# Quoted STRING
\"[^\"]*\" {set yylval [string range $yytext 1 end-1]; return $STRING_}

# Quoted STRING
\'[^\']*\' {set yylval [string range $yytext 1 end-1]; return $STRING_}

# Quoted STRING
\{[^\}]*\} {set yylval [string range $yytext 1 end-1]; return $STRING_}

# CMD
-\w+ {return $CMD_}

# STRING
\S+\S+ {set yylval $yytext; return $STRING_}

\s        # ignore whitespace

.	  {set yylval $yytext; return $yylval}

