.PHONY	: msgs clean

msgs	:
	grep 'msgcat::mc' $(SCRIPTS) | cut -d[ -f2 | sed -e 's/::mc/::mcset AAA /' -e 's/]/ ""/' | sort | uniq > ../msgs/tmpl.tcl

clean	:
	$(RM) core *~ *# 
